<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nabeel.auxilio">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIn" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUp" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".BecomeTutor" />
        <activity android:name=".Syllabus" />
        <activity android:name=".QuestionPaper" />
        <activity android:name=".Notes" />
        <activity android:name=".Colleges" />
        <activity android:name=".Classes" />
        <activity android:name=".ExamCourses" />
        <activity android:name=".FindTutor" />
        <activity android:name=".TutorDetail"></activity>
        <activity android:name=".activity.FingerprintActivity" />
    </application>

</manifest>